FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      f;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1e-10;

boundaryField
{
    frontAndBack
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    solidWall
    {
        type            fWallFunction;
        value           uniform 1e-10;
    }
    procBoundary6to0
    {
        type            processor;
        value           uniform 1e-10;
    }
    procBoundary6to1
    {
        type            processor;
        value           uniform 1e-10;
    }
    procBoundary6to4
    {
        type            processor;
        value           uniform 1e-10;
    }
    procBoundary6to7
    {
        type            processor;
        value           uniform 1e-10;
    }
    procBoundary6to14
    {
        type            processor;
        value           uniform 1e-10;
    }
    procBoundary6to15
    {
        type            processor;
        value           uniform 1e-10;
    }
}


