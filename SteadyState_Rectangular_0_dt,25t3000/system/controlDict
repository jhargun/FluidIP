FoamFile
{
    version     2.3;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict.simpleFoam;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       latestTime; //latestTime;

startTime       0;

stopAt          endTime;

endTime 3000;

deltaT .25;

writeControl    runTime;

writeInterval   1000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable false;


functions
{
    #include "fieldAverages.fun"
    #include "forceCoeffs.fun"
    #include "probes.fun"
}

