{
    frontAndBack
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    outlet
    {
        type            zeroGradient;
    }
    solidWall
    {
        type            zeroGradient;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 293;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 293;
    }
    procBoundary2to4
    {
        type            processor;
        value           uniform 293;
    }
    procBoundary2to5
    {
        type            processor;
        value           uniform 293;
    }
