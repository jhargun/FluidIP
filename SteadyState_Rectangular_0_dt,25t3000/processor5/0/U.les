FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U.les;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (90 0 0);

boundaryField
{
    frontAndBack
    {
        type            freestream;
        freestreamValue nonuniform 0();
        value           nonuniform 0();
    }
    inlet
    {
        type            freestream;
        freestreamValue nonuniform 0();
        value           nonuniform 0();
    }
    outlet
    {
        type            freestream;
        freestreamValue nonuniform 0();
        value           nonuniform 0();
    }
    solidWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    procBoundary5to0
    {
        type            processor;
        value           uniform (90 0 0);
    }
    procBoundary5to2
    {
        type            processor;
        value           uniform (90 0 0);
    }
    procBoundary5to4
    {
        type            processor;
        value           uniform (90 0 0);
    }
    procBoundary5to7
    {
        type            processor;
        value           uniform (90 0 0);
    }
}


